  <!-- Playwright settings -->
  <Playwright>
    <BrowserName>msedge</BrowserName>
    <LaunchOptions>
      <Headless>false</Headless>
      <Channel>msedge</Channel>
    </LaunchOptions>
  </Playwright>
</RunSettings>